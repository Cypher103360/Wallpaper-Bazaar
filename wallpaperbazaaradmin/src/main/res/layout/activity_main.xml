<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:padding="10dp"
            android:orientation="vertical">

            <Button
                android:id="@+id/pop_img_btn"
                android:layout_width="300dp"
                android:layout_height="56dp"
                android:layout_marginTop="16dp"
                android:text="Upload Popular Image"
                android:textAllCaps="false"
                android:textSize="18sp" />

            <Button
                android:id="@+id/new_img_btn"
                android:layout_width="300dp"
                android:layout_height="56dp"
                android:layout_marginTop="8dp"
                android:text="Upload Premium Images"
                android:textAllCaps="false"
                android:textSize="18sp" />

            <Button
                android:id="@+id/category_btn"
                android:layout_width="300dp"
                android:layout_height="56dp"
                android:layout_marginTop="8dp"
                android:text="@string/upload_category"
                android:textAllCaps="false"
                android:textSize="18sp" />

            <Button
                android:id="@+id/show_data_btn"
                android:layout_width="300dp"
                android:layout_height="56dp"
                android:layout_marginTop="8dp"
                android:text="@string/show_data"
                android:textAllCaps="false"
                android:textSize="18sp" />

            <Button
                android:id="@+id/upload_banner_btn"
                android:layout_width="300dp"
                android:layout_height="56dp"
                android:layout_marginTop="8dp"
                android:text="@string/upload_banner_image"
                android:textAllCaps="false"
                android:textSize="18sp" />

            <Button
                android:id="@+id/popular_btn"
                android:layout_width="300dp"
                android:layout_height="56dp"
                android:layout_marginTop="8dp"
                android:text="@string/show_popular_images"
                android:textAllCaps="false"
                android:textSize="18sp" />

            <Button
                android:id="@+id/premium_btn"
                android:layout_width="300dp"
                android:layout_height="56dp"
                android:layout_marginTop="8dp"
                android:text="@string/show_premium_images"
                android:textAllCaps="false"
                android:textSize="18sp" />
            <Button
                android:id="@+id/live_btn"
                android:layout_width="300dp"
                android:layout_height="56dp"
                android:layout_marginTop="8dp"
                android:text="@string/show_live_wallpapers"
                android:textAllCaps="false"
                android:textSize="18sp" />

            <Button
                android:id="@+id/ads_id_btn"
                android:layout_width="300dp"
                android:layout_height="56dp"
                android:layout_marginTop="8dp"
                android:text="@string/update_ads_ids"
                android:textAllCaps="false"
                android:textSize="18sp" />

            <Button
                android:id="@+id/hd_wallpaper_ads"
                android:layout_width="300dp"
                android:layout_height="56dp"
                android:layout_marginTop="8dp"
                android:text="HD Wallpapers Ads"
                android:textAllCaps="false"
                android:textSize="18sp" />

            <Button
                android:id="@+id/turbo_ads_btn"
                android:layout_width="300dp"
                android:layout_height="56dp"
                android:layout_marginTop="8dp"
                android:text="@string/turbo_share_ads_id"
                android:textAllCaps="false"
                android:textSize="18sp" />

            <Button
                android:id="@+id/user_data_btn"
                android:layout_width="300dp"
                android:layout_height="56dp"
                android:layout_marginTop="8dp"
                android:text="@string/show_user_data"
                android:textAllCaps="false"
                android:textSize="18sp" />

            <Button
                android:id="@+id/pro_wall_url_btn"
                android:layout_width="300dp"
                android:layout_height="56dp"
                android:layout_marginTop="8dp"
                android:text="@string/update_pro_wall_url"
                android:textAllCaps="false"
                android:textSize="18sp" />
            <Button
                android:id="@+id/get_free_wall_btn"
                android:layout_width="300dp"
                android:layout_height="56dp"
                android:layout_marginTop="8dp"
                android:text="Get Free Wallpaper App"
                android:textAllCaps="false"
                android:textSize="18sp" />
            <Button
                android:id="@+id/pro_File_Share_url_btn"
                android:layout_width="300dp"
                android:layout_height="56dp"
                android:layout_marginTop="8dp"
                android:text="@string/update_pro_file_url"
                android:textAllCaps="false"
                android:textSize="18sp" />
            <Button
                android:id="@+id/upload_live_wallpaper"
                android:layout_width="300dp"
                android:layout_height="56dp"
                android:layout_marginTop="8dp"
                android:text="@string/upload_live_wallpaper"
                android:textAllCaps="false"
                android:textSize="18sp" />

            <Button
                android:id="@+id/upload_featured_items"
                android:layout_width="300dp"
                android:layout_height="56dp"
                android:layout_marginTop="8dp"
                android:text="Upload Featured Items"
                android:textAllCaps="false"
                android:textSize="18sp" />

            <Button
                android:id="@+id/show_featured_items"
                android:layout_width="300dp"
                android:layout_height="56dp"
                android:layout_marginTop="8dp"
                android:text="Show Featured Items"
                android:textAllCaps="false"
                android:textSize="18sp" />

            <Button
                android:id="@+id/update_text_urls"
                android:layout_width="300dp"
                android:layout_height="56dp"
                android:layout_marginTop="8dp"
                android:text="Update Text and Urls"
                android:textAllCaps="false"
                android:textSize="18sp" />

            <Button
                android:id="@+id/FS_news_reviews_details"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:text="FileShare News and Reviews"
                android:textAllCaps="false"
                android:textSize="18sp" />

            <Button
                android:id="@+id/show_FS_news_reviews_details"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:text="Show News or Review Details"
                android:textAllCaps="false"
                android:textSize="18sp" />

        </LinearLayout>

    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>